<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>PicSorterNS</RootNamespace>
    <UseWPF>true</UseWPF>
    <RunAnalyzersDuringLiveAnalysis>False</RunAnalyzersDuringLiveAnalysis>
    <EnableNETAnalyzers>True</EnableNETAnalyzers>
    <AnalysisLevel>6.0-recommended</AnalysisLevel>
  </PropertyGroup>

  <ItemGroup>
    <Import Include="System.Windows" />
    <Import Include="System.Windows.Controls" />
    <Import Include="System.Windows.Data" />
    <Import Include="System.Windows.Documents" />
    <Import Include="System.Windows.Input" />
    <Import Include="System.Windows.Media" />
    <Import Include="System.Windows.Media.Imaging" />
    <Import Include="System.Windows.Navigation" />
    <Import Include="System.Windows.Shapes" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Auto_WinFace\VbLibCore31-Auto_WinFace.vbproj" />
    <ProjectReference Include="..\HiddenWatermark\AsNuget-HiddenWatermark.csproj" />
    <ProjectReference Include="..\Process_Signature\Process_SigAndFace.vbproj" />
    <ProjectReference Include="..\Publish_Instagram\Publish_std2_Instagram.vbproj" />
    <ProjectReference Include="..\Publish_std2_Facebook\Publish_std2_Facebook.vbproj" />
    <ProjectReference Include="..\Taggers_OCR\VbLibCore31-Auto_WinOCR_lib.vbproj" />
    <ProjectReference Include="..\vblib46ram\vblib46ram.vbproj" />
    <ProjectReference Include="..\VbLibCore3-mediaDevices\VbLibCore3-mediaDevices.vbproj" />
    <ProjectReference Include="..\VbLibCore3\VbLibCore3-picSource.vbproj" />
    <ProjectReference Include="..\VbLib\VbLib.vbproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Autotags\" />
    <Folder Include="WindowsWithPages\HashTags\" />
    <Folder Include="WindowsWithPages\SlideShow\" />
    <Folder Include="WindowsWithPages\Search\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.NetAnalyzers" Version="7.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>