<Page x:Class="SettingsMisc"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PicSorterNS"
      mc:Ignorable="d" 
      Title="Miscellaneous " Loaded="Page_Loaded">
    <StackPanel Margin="5,5,5,5">
        <TextBlock HorizontalAlignment="Center" Text="Miscellaneous " FontSize="14" />
        <CheckBox Name="uiFullJSON" Content="Pokaż pełny JSON (z NULLami)" Margin="0,10,0,0" />

        <Button Content=" OK " HorizontalAlignment="Center" Margin="5,10,5,0" Click="uiOK_Click" />
    </StackPanel>
</Page>
